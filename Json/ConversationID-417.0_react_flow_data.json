        },
        {
            "id": "node_104",
            "type": "customNode",
            "data": {
                "label": "RiziaDLC.Ending_Coup_Reform == true || RiziaDLC.Ending_Coup_Sazon == true || RiziaDLC.Ending_Coup_Azaro == true || RiziaDLC.Ending_Coup_Toras == true \n\nnan"
