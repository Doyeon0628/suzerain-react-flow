        },
        {
            "id": "node_97",
            "type": "customNode",
            "data": {
                "label": "BaseGameSupport.Journal_Turn04_Infrastructure_TaurusNoFunds = true\n\nnan"
            },
            "position": {
                "x": 0,
                "y": 0
            }
        },
        {
            "id": "node_106",
            "type": "customNode",
            "data": {
                "label": "Gus Manger\n\n“There are people I know in the management of Underhall. They will be available for a second project in the future.”"
            },
            "position": {
                "x": 0,
                "y": 0
            }
        },
        {
            "id": "node_107",
            "type": "customNode",
            "data": {
                "label": "Gus Manger\n\n“Not the fossil of a company. If anybody will have timeline issues it is them.”"
            },
            "position": {
                "x": 0,
                "y": 0
            }
        },
