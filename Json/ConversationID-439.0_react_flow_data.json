            }
        },
        {
            "id": "node_356",
            "type": "customNode",
            "data": {
                "label": "RiziaDLC.Policy_Economy_MorellaPipeline_Project == true || RiziaDLC.Policy_Economy_MorellaPipeline_Completed == true\n\nnan"
            },
            "position": {
